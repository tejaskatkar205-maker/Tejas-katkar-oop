 1a

#include <iostream>
#include <string>
using namespace std;

class Student {
    int roll_no;
    string name, cls;
public:
    void accept() {
        cin >> roll_no;
        cin.ignore();
        getline(cin, name);
        getline(cin, cls);
    }
    void display() {
        cout << roll_no << " " << name << " " << cls;
    }
};

int main() {
    Student s;
    s.accept();
    s.display();
}


1b

#include <iostream>
#include <string>
using namespace std;

class Book {
public:
    string name;
    int pages;
    double price;
    void accept() {
        getline(cin, name);
        cin >> price >> pages;
        cin.ignore();
    }
};

int main() {
    Book b1, b2;
    b1.accept();
    b2.accept();
    if (b1.price >= b2.price) cout << b1.name;
    else cout << b2.name;
}


1c

#include <iostream>
using namespace std;

class Time {
    int h, m, s;
public:
    void read() {
        char c1, c2;
        cin >> h >> c1 >> m >> c2 >> s;
    }
    int toSeconds() { return h * 3600 + m * 60 + s; }
};

int main() {
    Time t;
    t.read();
    cout << t.toSeconds();
}


2a

#include <iostream>
#include <string>
using namespace std;

class City {
public:
    string name;
    long population;
    void accept() {
        getline(cin, name);
        cin >> population;
        cin.ignore();
    }
};

int main() {
    City c[5];
    for (int i = 0; i < 5; i++) c[i].accept();
    int idx = 0;
    for (int i = 1; i < 5; i++) if (c[i].population > c[idx].population) idx = i;
    cout << c[idx].name;
}


2b

#include <iostream>
#include <string>
using namespace std;

class Account {
public:
    string accno;
    double bal;
    void accept() { cin >> accno >> bal; }
    void interest() { if (bal >= 5000) bal = bal * 1.10; }
    void show() { cout << accno << " " << bal << "\n"; }
};

int main() {
    Account a[10];
    for (int i = 0; i < 10; i++) a[i].accept();
    for (int i = 0; i < 10; i++) { a[i].interest(); a[i].show(); }
}


2c

#include <iostream>
#include <string>
using namespace std;

class Staff {
public:
    string name, post;
    void accept() { getline(cin, name); getline(cin, post); }
};

int main() {
    Staff s[5];
    for (int i = 0; i < 5; i++) s[i].accept();
    for (int i = 0; i < 5; i++) if (s[i].post == "HOD") cout << s[i].name << "\n";
}


3a

#include <iostream>
#include <string>
using namespace std;

class Book {
    string title, author;
    double price;
public:
    void accept() { getline(cin, title); getline(cin, author); cin >> price; }
    void show() { cout << title << " " << author << " " << price; }
};

int main() {
    Book b, *p = &b;
    p->accept();
    p->show();
}


