#include <iostream>
using namespace std;

class A {
public:
    void showA() { cout << "A "; }
};

class B : public A {
public:
    void showB() { cout << "B "; }
};

class C : public B {
public:
    void showC() { cout << "C "; }
};

int main() {
    C c;
    c.showA();
    c.showB();
    c.showC();
}


