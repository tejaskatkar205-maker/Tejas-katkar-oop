#include <fstream>
using namespace std;

int main() {
    ifstream f1("First.txt");
    ofstream f2("Second.txt");
    char ch;
    while (f1.get(ch))
        f2.put(ch);
}


9b

#include <iostream>
#include <fstream>
using namespace std;

int main() {
    ifstream f("file.txt");
    char c;
    int d = 0, s = 0;
    while (f.get(c)) {
        if (isdigit(c)) d++;
        if (isspace(c)) s++;
    }
    cout << d << " " << s;
}


