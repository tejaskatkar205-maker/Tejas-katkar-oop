#include <iostream>
using namespace std;

class Student {
    int roll;
    double per;
public:
    Student& accept() { cin >> roll >> per; return *this; }
    Student& display() { cout << roll << " " << per; return *this; }
};

int main() {
    Student().accept().display();
}


3c

#include <iostream>
using namespace std;

class Outer {
    int secret;
public:
    Outer() { secret = 100; }
    class Inner {
    public:
        void show(Outer& o) { cout << o.secret; }
    };
};

int main() {
    Outer o;
    Outer::Inner in;
    in.show(o);
}
4a

#include <iostream>
using namespace std;

class Result1 {
public:
    int m;
    void get() { cin >> m; }
};

class Result2 {
public:
    int m;
    void get() { cin >> m; }
};

float avg(Result1 a, Result2 b) {
    return (a.m + b.m) / 2.0;
}

int main() {
    Result1 r1;
    Result2 r2;
    r1.get();
    r2.get();
    cout << avg(r1, r2);
}


4b

#include <iostream>
using namespace std;

class B;

class A {
    int n;
public:
    A(int x = 0) { n = x; }
    friend int big(A, B);
};

class B {
    int n;
public:
    B(int x = 0) { n = x; }
    friend int big(A, B);
};

int big(A a, B b) {
    return a.n > b.n ? a.n : b.n;
}

int main() {
    int x, y;
    cin >> x >> y;
    A a(x);
    B b(y);
    cout << big(a, b);
}


4c

#include <iostream>
using namespace std;

class Sample {
    int v;
public:
    Sample(int x = 0) { v = x; }
    friend void swapv(Sample&, Sample&);
    void show() { cout << v << " "; }
};

void swapv(Sample& a, Sample& b) {
    int t = a.v;
    a.v = b.v;
    b.v = t;
}

int main() {
    Sample s1(5), s2(10);
    swapv(s1, s2);
    s1.show();
    s2.show();
}
5a

#include <iostream>
using namespace std;

class Sum {
    int n, s;
public:
    Sum(int x) {
        n = x;
        s = 0;
        for (int i = 1; i <= n; i++)
            s += i;
        cout << s;
    }
};

int main() {
    int n;
    cin >> n;
    Sum a(n);
}


5b

#include <iostream>
using namespace std;

class Student {
    string n;
    float p;
public:
    Student(string a, float b) {
        n = a;
        p = b;
    }
    void show() {
        cout << n << " " << p;
    }
};

int main() {
    string n;
    float p;
    cin >> n >> p;
    Student s(n, p);
    s.show();
}


5c

#include <iostream>
using namespace std;

class College {
    int roll;
    string name, course;
public:
    College(int r, string n, string c = "Computer Engineering") {
        roll = r;
        name = n;
        course = c;
    }
    void show() {
        cout << roll << " " << name << " " << course << endl;
    }
};

int main() {
    College a(1, "A"), b(2, "B", "IT");
    a.show();
    b.show();
}


5d

#include <iostream>
using namespace std;

class Demo {
    int x;
public:
    Demo() {
        x = 0;
        cout << x << endl;
    }
    Demo(int a) {
        x = a;
        cout << x << endl;
    }
    Demo(Demo &d) {
        x = d.x;
        cout << x << endl;
    }
};

int main() {
    Demo a, b(5), c(b);
}


