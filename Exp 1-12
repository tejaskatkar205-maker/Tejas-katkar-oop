6d

#include <iostream>
using namespace std;

class A {
public:
    void showA() { cout << "A "; }
};

class B : virtual public A {
public:
    void showB() { cout << "B "; }
};

class C : virtual public A {
public:
    void showC() { cout << "C "; }
};

class D : public B, public C {
public:
    void showD() { cout << "D "; }
};

int main() {
    D d;
    d.showA();
    d.showB();
    d.showC();
    d.showD();
}
7a

#include <iostream>
using namespace std;

void area(int l, int b) {
    cout << l * b;
}

void area(int s) {
    cout << s * s;
}

int main() {
    int l, b;
    cin >> l >> b;
    area(l, b);
}


7b

#include <iostream>
using namespace std;

void sum(int a, int b) {
    cout << a + b;
}

void sum(int a, int b, int c) {
    cout << a + b + c;
}

int main() {
    int x, y, z;
    cin >> x >> y >> z;
    sum(x, y, z);
}


7c

#include <iostream>
using namespace std;

class Test {
    int n;
public:
    Test(int a) { n = a; }
    void operator -() { n = -n; }
    void show() { cout << n; }
};

int main() {
    Test t(5);
    -t;
    t.show();
}


7d

#include <iostream>
using namespace std;

class Num {
    int n;
public:
    Num(int a) { n = a; }
    void operator ++() { ++n; }
    void operator ++(int) { n++; }
    void show() { cout << n; }
};

int main() {
    Num a(5);
    a++;
    ++a;
    a.show();
}


8a

#include <iostream>
#include <string>
using namespace std;

class Concat {
    string s;
public:
    Concat(string a = "") { s = a; }
    Concat operator +(Concat b) {
        return Concat(s + b.s);
    }
    void show() { cout << s; }
};

int main() {
    Concat a("xyz"), b("pqr"), c;
    c = a + b;
    c.show();
}


8b

#include <iostream>
#include <string>
using namespace std;

class ILogin {
protected:
    string name, pwd;
public:
    virtual void accept() {
        cin >> name >> pwd;
    }
};

class EmailLogin : public ILogin {
public:
    void accept() {
        cin >> name >> pwd;
    }
};

class MemberLogin : public ILogin {
public:
    void accept() {
        cin >> name >> pwd;
    }
};

int main() {
    EmailLogin e;
    MemberLogin m;
    e.accept();
    m.accept();
}


